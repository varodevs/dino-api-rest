{ 
    "version": 2, 
    "routes": [
        { 
            "src": "/", 
            "dest": "/api/v1"
        },
        { 
            "src": "/api/v1/dinos",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "/api/v1/dinos.js"
        },
        {
           "src": "/api/v1/random",
            "methods": [
                "POST"
            ],
            "dest": "/api/v1/random.js" 
        },
        {
           "src": "/api/v1/sorted",
            "methods": [
                "GET"
            ],
            "dest": "/api/v1/sorted.js" 
        },
        {
           "src": "/api/v1/dinos/:name",
            "methods": [
                "GET"
            ],
            "dest": "/api/v1/dinos/:name" 
        },
        {
           "src": "/api/v1/dinos/by-id/:id",
            "methods": [
                "GET"
            ],
            "dest": "/api/v1/dinos/by-id/:id" 
        }
    ] 
}